# Default values for local development
# For production, update these URLs to your deployed domain

# Required for Vercel deployment
NEXT_PUBLIC_URL='http://localhost:3000'
NEXTAUTH_URL='http://localhost:3000'
NEXTAUTH_SECRET='your_nextauth_secret_here'

# Neynar API Keys (Required for Farcaster)
NEXT_PUBLIC_NEYNAR_CLIENT_ID='your_neynar_client_id'
NEXT_PUBLIC_NEYNAR_CLIENT_SECRET='your_neynar_client_secret'
NEYNAR_API_KEY='your_neynar_api_key'

# Database Configuration (Required for production)
DATABASE_URL='postgresql://username:password@hostname:port/database'

# Farcaster Configuration
FARCASTER_DEVELOPER_MNEMONIC='your_farcaster_developer_mnemonic'
FARCASTER_DEVELOPER_FID='your_farcaster_developer_fid'

# Optional Configuration
USE_TUNNEL='true'  # Set to 'false' in production

# Caching (Optional)
KV_REST_API_URL=''
KV_REST_API_TOKEN=''

# Zora Configuration (Optional)
NEXT_PUBLIC_ZORA_API_KEY='your_zora_api_key_here'

# Vercel Environment (Automatically set by Vercel)
VERCEL_ENV='development'  # 'production' in production
VERCEL_URL=''  # Will be set by Vercel
VERCEL_GIT_COMMIT_SHA=''  # Will be set by Vercel
